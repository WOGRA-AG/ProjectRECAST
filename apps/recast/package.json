{
  "name": "recast",
  "version": "0.0.0",
  "scripts": {
    "generate:base": "openapi-generator-cli generate -g typescript-angular --generator-key v3.0 --additional-properties=ngVersion=14.2.0",
    "generate:api": "npm run generate:base -- -i ../oas/recast-models.yaml -o build/openapi/recast",
    "prestart": "npm run generate:api",
    "prebuild": "npm run generate:api",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-dev": "npm run prebuild && ng build --configuration development",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint-staged": "prettier --write .",
    "e2e": "npx cypress run",
    "prepare": "cd ../.. && husky install ./apps/recast/.husky",
    "i18n": "ng extract-i18n",
    "config-prettier": "eslint-config-prettier src/main.ts"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^14.1.0",
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.6",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.12",
    "@angular/localize": "^14.2.12",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "@supabase/supabase-js": "^2.7.1",
    "node-sass-json-importer": "^4.3.0",
    "rxjs": "~7.5.0",
    "sass": "^1.58.0",
    "snakecase-keys": "^5.4.5",
    "tslib": "^2.5.0",
    "yaml": "^2.2.1",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "~14.2.7",
    "@angular/compiler-cli": "^14.2.0",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/jasmine": "~4.0.0",
    "@types/snakecase-keys": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "cypress": "^12.5.1",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.9.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-rxjs-angular": "^2.0.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.8.4",
    "typescript": "~4.9.5"
  }
}
