{
  "name": "recast",
  "version": "0.0.0",
  "scripts": {
    "generate:base": "openapi-generator-cli generate -g typescript-angular --generator-key v3.0 --additional-properties=ngVersion=14.2.0",
    "generate:api": "npm run generate:base -- -i ../oas/recast-models.yaml -o build/openapi/recast",
    "prestart": "npm run generate:api",
    "prebuild": "npm run generate:api",
    "ng": "ng",
    "start": "ng serve",
    "start-dev": "npm run start -- --host 0.0.0.0",
    "build": "ng build",
    "build-dev": "npm run prebuild && ng build --configuration development",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint --fix && prettier --write .",
    "lint:check": "ng lint && prettier --check .",
    "e2e": "npx cypress run",
    "e2e:local": "cross-env CYPRESS_dev=true npx cypress run",
    "e2e:chrome": "npm run e2e -- --browser chrome",
    "e2e:firefox": "npm run e2e -- --browser firefox",
    "e2e:edge": "npm run e2e -- --browser edge",
    "prepare": "cd ../.. && husky install ./apps/recast/.husky",
    "i18n": "ng extract-i18n",
    "config-prettier": "eslint-config-prettier src/main.ts"
  },
  "overrides": {
    "d3-selection": "3.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.1",
    "@angular/cdk": "^16.1.1",
    "@angular/common": "^16.1.1",
    "@angular/compiler": "^16.1.1",
    "@angular/core": "^16.1.1",
    "@angular/forms": "^16.1.5",
    "@angular/localize": "^16.1.5",
    "@angular/material": "^16.1.1",
    "@angular/material-moment-adapter": "^16.1.5",
    "@angular/platform-browser": "^16.1.1",
    "@angular/platform-browser-dynamic": "^16.1.5",
    "@angular/router": "^16.1.5",
    "@dlr-shepard/shepard-client": "2023.4.11",
    "@supabase/supabase-js": "^2.26.0",
    "@swimlane/ngx-graph": "^8.2.1",
    "buffer": "^6.0.3",
    "cross-env": "7.0.3",
    "csv-parse": "^5.5.0",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-sass-json-importer": "^4.3.0",
    "rxjs": "~7.8.0",
    "sass": "^1.63.6",
    "snakecase-keys": "^5.4.6",
    "tslib": "^2.6.0",
    "yaml": "^2.3.1",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.4",
    "@angular-eslint/builder": "16.0.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/schematics": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~16.1.0",
    "@angular/compiler-cli": "^16.1.1",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/jasmine": "~4.3.1",
    "@types/jszip": "^3.4.1",
    "@types/lodash": "^4.14.195",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cypress": "^12.17.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.3.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-rxjs-angular": "^2.0.1",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.8.8",
    "typescript": "~4.9.5"
  }
}
