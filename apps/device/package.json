{
  "name": "recast upload client",
  "private": true,
  "version": "1.0.0",
  "description": "A client to upload files for RECAST framework.",
  "main": "index.js",
  "scripts": {
    "build": "tsc --build",
    "lint": "tsc --noEmit && eslint \"**/*.ts\" --fix && prettier --write .",
    "lint:check": "tsc --noEmit && eslint \"**/*.ts\" && prettier --check .",
    "serve": "ts-node ./src",
    "start": "node ./dist",
    "test": "mocha -r ts-node/register **/test/**/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WOGRA-AG/recast.git"
  },
  "keywords": [
    "RECAST",
    "supabase",
    "client"
  ],
  "author": "WOGRA AG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WOGRA-AG/recast/issues"
  },
  "homepage": "https://github.com/WOGRA-AG/recast#readme",
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.5.0",
    "chokidar": "^3.5.3",
    "dotenv": "^16.0.3",
    "mkdirp": "^2.1.3",
    "rimraf": "^4.1.2"
  }
